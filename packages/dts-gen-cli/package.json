{
  "name": "dts-gen-cli",
  "description": "A CLI to generate TypeScript types from React prop types",
  "version": "0.0.0",
  "author": "= @leandrotk",
  "bin": {
    "dts-gen-cli": "./bin/run"
  },
  "bugs": "https://github.com/leandrotk/laziness/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@types/node": "^10",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "eslint": "^7.23.0",
    "globby": "^10",
    "prettier": "^2.2.1",
    "ts-node": "^8",
    "typescript": "^4.2.4"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/leandrotk/laziness",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "dts-gen-cli",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "leandrotk/laziness",
  "scripts": {
    "start": "./bin/run hello",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
